{
  "name": "next-starter",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "run-s -l -c --aggregate-output clean build-next",
    "export": "run-s -l -c --aggregate-output clean build-next export-next",
    "build-next": "next build",
    "export-next": "next export",
    "clean": "rimraf .next out",
    "start": "next start",
    "lint": "run-p -l -c --aggregate-output lint:*",
    "lint:eslint": "eslint --cache .",
    "lint:prettier": "prettier --check .",
    "fix": "run-s -l -c fix:eslint fix:prettier",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn lint:prettier --write",
    "test": "run-p -l -c --aggregate-output lint:* unit",
    "unit": "jest",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006 -s ./mocks/data",
    "build-storybook": "build-storybook"
  },
  "babel": {
    "presets": [
      "next/babel"
    ]
  },
  "prettier": {
    "printWidth": 120
  },
  "dependencies": {
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "6.3.12",
    "@storybook/addon-actions": "6.3.12",
    "@storybook/addon-essentials": "6.3.12",
    "@storybook/addon-links": "6.3.12",
    "@storybook/react": "6.3.12",
    "@swc/core": "1.2.102",
    "@swc/jest": "0.2.5",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "7.0.2",
    "@types/gtag.js": "0.0.7",
    "@types/jest": "27.0.2",
    "@types/node": "16.0.0",
    "@types/react": "17.0.32",
    "@types/react-test-renderer": "17.0.1",
    "@types/testing-library__react": "10.2.0",
    "@typescript-eslint/eslint-plugin": "5.1.0",
    "@typescript-eslint/parser": "5.1.0",
    "eslint": "8.1.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "husky": "7.0.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.3.1",
    "jest-watch-typeahead": "1.0.0",
    "lint-staged": "11.2.4",
    "msw": "0.35.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.4.1",
    "react-error-boundary": "3.1.3",
    "react-test-renderer": "17.0.2",
    "rimraf": "3.0.2",
    "storybook-addon-performance": "0.16.1",
    "typescript": "4.4.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": "yarn fix:eslint",
    "*": "yarn fix:prettier"
  }
}
