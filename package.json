{
  "name": "next-starter",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "serve": "run-s build start",
    "lint": "next lint",
    "prettier": "prettier --cache --check .",
    "unit": "jest",
    "e2e": "playwright test",
    "test": "run-p -l -c --aggregate-output lint prettier unit",
    "fix": "run-s -l -c fix:lint fix:prettier",
    "fix:lint": "next lint --fix",
    "fix:prettier": "prettier --cache --write .",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "13.4.12",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.10",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@babel/core": "7.22.9",
    "@playwright/test": "1.36.2",
    "@storybook/addon-a11y": "7.2.1",
    "@storybook/addon-actions": "7.2.1",
    "@storybook/addon-essentials": "7.2.1",
    "@storybook/addon-interactions": "7.2.1",
    "@storybook/addon-links": "7.2.1",
    "@storybook/builder-webpack5": "7.2.1",
    "@storybook/manager-webpack5": "6.5.16",
    "@storybook/react": "7.2.1",
    "@storybook/testing-library": "0.2.0",
    "@storybook/testing-react": "2.0.0",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "14.0.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@types/gtag.js": "0.0.12",
    "@types/jest": "29.5.3",
    "@types/node": "17.0.35",
    "@types/react": "18.2.18",
    "@types/react-test-renderer": "18.0.0",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "babel-loader": "9.1.3",
    "eslint": "8.46.0",
    "eslint-config-next": "13.4.13",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sort-destructure-keys": "1.5.0",
    "eslint-plugin-storybook": "0.6.13",
    "eslint-plugin-testing-library": "5.11.1",
    "husky": "8.0.3",
    "jest": "29.6.2",
    "jest-environment-jsdom": "29.6.2",
    "lint-staged": "13.2.3",
    "msw": "1.2.3",
    "npm-run-all": "4.1.5",
    "prettier": "3.0.1",
    "react-test-renderer": "18.2.0",
    "scaffdog": "3.0.0",
    "storybook-addon-next": "1.8.0",
    "storybook-addon-performance": "0.17.1",
    "storybook-dark-mode": "3.0.1",
    "typescript": "5.1.6"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "next lint --fix .",
      "prettier --write ."
    ],
    "**/*.{json,md,mdx,css,html,yml,yaml,scss}": [
      "prettier --write ."
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
